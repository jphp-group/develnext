               <padding>
                  <Insets left="15.0" right="15.0" />
               </padding>
            </Button>
            <TextField id="queryField" prefHeight="39.0" prefWidth="397.0" promptText="введите ключевые слова для поиска" HBox.hgrow="ALWAYS">
